{
	"name": "nestegg",
	"version": "1.0.0",
	"description": "Loan application",
	"main": "index.ts",
	"scripts": {
		"start": "ts-node src/server.ts",
		"dev": "nodemon -r dotenv/config src/server.ts",
		"migration:generate": "sequelize-cli migration:generate --migrations-path src/migrations --name",
		"migration:up": "sequelize-cli db:migrate --config src/config/database/index.js --migrations-path src/migrations",
		"migration:down": "sequelize-cli db:migrate:undo --config src/config/database/index.js --models-path src/models --migrations-path src/migrations",
		"test": "jest --config jest.config.js"
	},
	"dependencies": {
		"@types/supertest": "^6.0.2",
		"bcryptjs": "^3.0.2",
		"bignumber.js": "^9.1.2",
		"compression": "^1.8.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-validation": "^4.1.1",
		"helmet": "^8.1.0",
		"init": "^0.1.2",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.14.1",
		"pg-hstore": "^2.3.4",
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0",
		"sequelize": "^6.37.6",
		"supertest": "^7.1.0",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/express": "4.17.21",
		"@types/express-serve-static-core": "4.19.6",
		"@types/jest": "^29.5.14",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/sequelize": "^4.28.20",
		"@typescript-eslint/eslint-plugin": "^8.27.0",
		"@typescript-eslint/parser": "^8.27.0",
		"jest": "^29.7.0",
		"nodemon": "^3.1.9",
		"sequelize-cli": "^6.6.2",
		"ts-jest": "^29.3.0",
		"ts-node": "^10.9.2",
		"typescript": "5.7.3"
	}
}
